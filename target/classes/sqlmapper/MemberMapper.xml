<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="lib.member.model.MemberDAO">
	
	<!-- 권한변경 -->
	<update id="memberRankUpdate" 
		parameterType="lib.member.model.MemberDTO">
		update member set mem_rank = #{mem_rank} where mem_id = #{mem_id}
	</update>

	<!-- 리스트(selectAll) -->
	<select id="memberSelectAll" 
		resultType="lib.member.model.MemberDTO">
		select * from member
	</select>

	<!-- ID 검색(select where) -->
	<select id="memberSearchID"
		resultType="lib.member.model.MemberDTO">
		select * from member where mem_id = #{mem_id}
	</select>

</mapper>



